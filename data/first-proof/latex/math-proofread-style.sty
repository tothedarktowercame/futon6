\newcommand{\MathProofreadColorLegend}{%
  \begingroup
  \renewcommand{\arraystretch}{1.2}%
  \setlength{\tabcolsep}{8pt}%
  \begin{center}
  \begin{tabular}{>{\raggedright\arraybackslash}p{0.32\linewidth}>{\raggedright\arraybackslash}p{0.18\linewidth}>{\raggedright\arraybackslash}p{0.42\linewidth}}
    \textbf{Syntax class} & \textbf{Colour} & \textbf{Examples} \\
    Greek letters & \textcolor{MPSyntaxGreekColor}{Mulberry} & $\mGreek{\alpha},\,\mGreek{\beta},\,\mGreek{\Gamma}$ \\
    Binary operators & \textcolor{MPSyntaxOperatorColor}{Purple} & $x\mOperator{\times}y,\,U\mOperator{\cup}V,\,A\mOperator{\otimes}B$ \\
    Additive/product bridges & \textcolor{MPSyntaxBridgeOperatorColor}{SeaGreen} & $a+b,\,u\mBridgeOperator{\ast}v$ \\
    Named operators & \textcolor{MPSyntaxNamedOperatorColor}{BurntOrange} & $\mOpName{span}(v_1,v_2),\,\mOpName{ker}(T),\,\mOpName{rank}(A),\,\mOpName{dim}(V)$ \\
    Dual/cotangent marker & \textcolor{MPSyntaxDualMarkerColor}{Turquoise} & $T^{\mDualStar}M,\,E^{\mDualStar}$ \\
    Comparison operators & \textcolor{MPSyntaxComparisonColor}{BritishRacingGreen} & $x\mCompare{=}y,\,x\mCompare{\le}y,\,x\mCompare{\neq}y,\,x\mCompare{\approx}y$ \\
    Relation operators & \textcolor{MPSyntaxRelationColor}{RoyalPurple} & $x\mRel{\in}S,\,A\mRel{\subseteq}B$ \\
    Large operators & \textcolor{MPSyntaxLargeOperatorColor}{BlueViolet} & $\mLargeOperator{\sum},\,\mLargeOperator{\prod},\,\mLargeOperator{\int},\,\mLargeOperator{\lim}$ \\
    Arrows & \textcolor{MPSyntaxArrowColor}{TealBlue} & $f\mArrow{\to}g,\,P\mArrow{\Rightarrow}Q,\,x\mArrow{\mapsto}x^2,\,A\mArrow{\leftrightarrow}B$ \\
    Delimiters & \textcolor{MPSyntaxDelimiterColor}{Magenta} & $\mDelimiter{(}x\mDelimiter{)},\,\mDelimiter{[}x\mDelimiter{]},\,\mDelimiter{\{}x\mDelimiter{\}},\,\langle x\rangle,\,|x|$ \\
    Functions & \textcolor{MPSyntaxFunctionColor}{Orchid} & $\mFunction{\sin}x,\,\mFunction{\cos}x,\,\mFunction{\log}x,\,\mFunction{\exp}x,\,\mFunction{\det}A$ \\
    Numbers/constants & \textcolor{MPSyntaxNumberColor}{Red} & $\mNumber{0},\ldots,\mNumber{9},\,\mConstant{e}$ \\
  \end{tabular}
  \end{center}
  \endgroup
}

