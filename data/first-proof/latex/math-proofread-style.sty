\newcommand{\MP@ActivateRawMathChars}{%
  \ifMPRawMathCharsActivated\else
    \MPRawMathCharsActivatedtrue
    \mathcode`<="8000
    \mathcode`>="8000
    \mathcode`=="8000
    \mathcode`+="8000
    \mathcode`[="8000
    \mathcode`]="8000
  \fi
}

\newcommand{\MP@DeactivateRawMathChars}{%
  \ifMPRawMathCharsActivated
    \MPRawMathCharsActivatedfalse
    \mathcode`<="313C
    \mathcode`>="313E
    \mathcode`=="303D
    \mathcode`+="202B
    \mathcode`[="405B
    \mathcode`]="505D
  \fi
}

