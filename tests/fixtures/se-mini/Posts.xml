  <row Id="6" PostTypeId="1" CreationDate="2020-01-03T00:00:00.000" Score="2" ViewCount="60" Body="&lt;p&gt;Suppose $x+y=1$. Is there an inequality bound?&lt;/p&gt;" OwnerUserId="6" Title="Simple inequality" Tags="|inequalities|real-analysis|" AnswerCount="2" AcceptedAnswerId="7" />
  <row Id="7" PostTypeId="2" ParentId="6" CreationDate="2020-01-03T01:00:00.000" Score="5" Body="&lt;p&gt;By Cauchy-Schwarz, $(x+y)^2 \le 2(x^2+y^2)$.&lt;/p&gt;" OwnerUserId="7" />
  <row Id="8" PostTypeId="2" ParentId="6" CreationDate="2020-01-03T02:00:00.000" Score="0" Body="&lt;p&gt;Try a simpler case first and check equality at $x=y$.&lt;/p&gt;" OwnerUserId="8" />

